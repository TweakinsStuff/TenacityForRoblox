local Mercury = loadstring(game:HttpGet("https://raw.githubusercontent.com/deeeity/mercury-lib/master/src.lua"))()

local GUI = Mercury:Create{
    Name = "Tenacity",
    Size = UDim2.fromOffset(600, 400),
    Theme = Mercury.Themes.Dark,
    Link = "https://github.com/deeeity/mercury-lib"
}

local Tab = GUI:Tab{
	Name = "Combat",
	Icon = "rbxassetid://8569322835"
}

Tab:Button{
	Name = "Sprint",
	Description = AutoSprint,
	Callback = function()-- Load the LuaSocket library
local socket = require("socket")

-- Set the key code for left shift
local key_code = 0x2A

-- Set the repeat interval (in milliseconds)
local repeat_interval = 10

-- Connect to the keyboard input device
local keyboard = assert(socket.tcp())
keyboard:connect("127.0.0.1", 4444)

-- Send the key press event
keyboard:send(string.char(0x01, key_code, 0x01))

-- Loop forever, sending key repeat events
while true do
  socket.sleep(repeat_interval / 1000)
  keyboard:send(string.char(0x01, key_code, 0x02))
end end
}
